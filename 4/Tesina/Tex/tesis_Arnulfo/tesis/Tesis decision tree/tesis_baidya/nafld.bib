	Journal = {Gastroenterology},
	Journal-Full = {Gastroenterology},
	Mesh = {Adult; Aged; Biopsy; Fatty Liver; Female; Hepacivirus; Humans; Iron; Liver; Liver Cirrhosis; Longitudinal Studies; Male; Middle Aged; RNA, Messenger; RNA, Viral; Survival Analysis},
	Month = {Jun},
	Number = {6},
	Pages = {1413-9},
	Pmid = {10348825},
	Pst = {ppublish},
	Title = {Nonalcoholic fatty liver disease: a spectrum of clinical and pathological severity},

	Doi = {10.1016/j.metabol.2010.03.006},
	Journal = {Metabolism},
	Journal-Full = {Metabolism: clinical and experimental},
	Mesh = {Adult; Biomarkers; Chromatography, High Pressure Liquid; Fatty Liver; Female; Gas Chromatography-Mass Spectrometry; Humans; Male; Metabolome; Metabolomics; Middle Aged; Non-alcoholic Fatty Liver Disease; Tandem Mass Spectrometry},
	Month = {Mar},

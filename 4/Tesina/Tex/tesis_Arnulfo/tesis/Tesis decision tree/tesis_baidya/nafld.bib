	Journal-Full = {Journal of hepatology},
	Mesh = {Carcinoma, Hepatocellular; Diabetes Complications; Fatty Liver; Humans; Liver Cirrhosis; Liver Neoplasms; Non-alcoholic Fatty Liver Disease; Obesity},

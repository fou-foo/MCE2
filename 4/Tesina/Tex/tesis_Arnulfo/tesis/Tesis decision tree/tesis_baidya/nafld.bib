	Journal = {Amino Acids},
	Journal-Full = {Amino acids},
	Mesh = {Carnitine; Female; Gene Expression Profiling; Humans; Isoleucine; Leucine; Male; Metabolomics; Non-alcoholic Fatty Liver Disease; Signal Transduction; Valine},
